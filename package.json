    "@fortawesome/ember-fontawesome": "^0.2.1",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@glimmer/component": "^1.0.0",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^3.4.1",
