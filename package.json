{
  "name": "ember-rollbar-client",
  "version": "0.10.0",
  "description": "The Rollbar client for Ember applications.",
  "keywords": [
    "ember-addon",
    "rollbar",
    "client",
    "logger",
    "ember"
  ],
  "repository": "https://github.com/Exelord/ember-rollbar-client",
  "license": "MIT",
  "author": "Exelord (Maciej KwaÅ›niak)",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
