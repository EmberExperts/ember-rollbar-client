{
  "name": "ember-rollbar-client",
  "version": "0.10.0",
  "description": "The Rollbar client for Ember applications.",
  "keywords": [
    "ember-addon",
    "rollbar",
    "client",
    "logger",
    "ember"
  ],
  "repository": "https://github.com/Exelord/ember-rollbar-client",
  "license": "MIT",
  "author": "Exelord (Maciej KwaÅ›niak)",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember server",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "release-it": {
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "git": {
      "tagName": "v${version}"
    }
  },
  "peerDependencies": {
    "rollbar": ">= 2.19.4"
  },
  "dependencies": {
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.23.1",
    "git-repo-version": "^1.0.2"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.1.4",
    "@glimmer/component": "^1.0.3",
    "@glimmer/tracking": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.24.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~3.24.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.3.0",
    "rollbar": "~2.19.4"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://exelord.github.io/ember-rollbar-client/"
  }
}
