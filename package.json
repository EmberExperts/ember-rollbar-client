  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://exelord.github.io/ember-rollbar-client/"
  }
